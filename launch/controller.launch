<!-- This will start the controller. -->
<launch>
  <arg name="vicon" default="false" />
  <rosparam command="load" file="$(find drone_dev)/config/common.yaml" ns="/dji2" />
  <node name="controller" pkg="drone_dev" type="pycontroller.py" args="--vicon __ns:=/dji2" output="screen" if="$(arg vicon)" />
  <node name="controller" pkg="drone_dev" type="pycontroller.py" args="__ns:=/dji2" output="screen" unless="$(arg vicon)" />
</launch>

